{
  "name": "wonder-scraper-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean:temp": "rm -f vite.config.timestamp_*.js app.config.timestamp_*.js",
    "dev": "npm run clean:temp && vite",
    "build": "npm run clean:temp && tsc --noEmit && vite build",
    "build:vercel": "npm run clean:temp && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No linter configured'",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@tanstack/react-query": "^5.51.0",
    "@tanstack/react-router": "^1.45.0",
    "@tanstack/react-table": "^8.19.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/og": "^0.8.5",
    "clsx": "^2.1.1",
    "ky": "^1.4.0",
    "lucide-react": "^0.424.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.4.1",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.2.0",
    "tailwind-merge": "^2.4.0"
