{
  "buildCommand": "npm install && cd frontend && npm install && npm run build:vercel",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/**/*.tsx": {
      "runtime": "@vercel/edge"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}

